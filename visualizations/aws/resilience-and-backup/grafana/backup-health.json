          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "--SELECT COUNT(resource_arn), resource_type \n--FROM aws_backup_protected_resources \n--GROUP BY resource_type LIMIT 50;\n\nDROP TABLE if EXISTS resource_count; \n\nCREATE TABLE resource_count (\n\tcounted int,\n\tresource_name varchar(255)\n);\n\nINSERT INTO resource_count(counted, resource_name)\nSELECT COUNT(arn), 'S3' FROM aws_s3_buckets;\n\nINSERT INTO resource_count(counted, resource_name)\nSELECT COUNT(arn), 'EC2' FROM aws_ec2_instances;\n\nINSERT INTO resource_count(counted, resource_name)\nSELECT COUNT(arn), 'DynamoDB' FROM aws_dynamodb_tables;\n\n    SELECT resource_type, backupcount as complaintresources, counted-backupcount as unprotectedresources from\n    ( SELECT COUNT(resource_arn) as backupcount, resource_type\n    FROM aws_backup_protected_resources \n    GROUP BY resource_type \n    ) as backup_numbers\n  LEFT JOIN resource_count\n  ON backup_numbers.resource_type = resource_count.resource_name\n  WHERE resource_type = 'DynamoDB'\n  ;\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "DynamoDB Backup Compliance",
      "type": "piechart"
    },
